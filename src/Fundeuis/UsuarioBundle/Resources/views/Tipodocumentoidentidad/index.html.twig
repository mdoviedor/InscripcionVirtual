{% block contenido %} 
    <table class="table tab-content table-hover table-striped records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('administrador_configuracion_tipodocumentoidentidad_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.nombre }}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('administrador_configuracion_tipodocumentoidentidad_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('administrador_configuracion_tipodocumentoidentidad_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('administrador_configuracion_tipodocumentoidentidad_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
{% endblock %}
