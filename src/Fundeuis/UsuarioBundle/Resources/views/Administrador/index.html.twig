{% extends "::superadministrador.html.twig" %}
{% block titulo %}
    <h1>Administrador list</h1>
{% endblock %}
{% block submenuizquierdo %}
{% endblock %}
{% block submenuderecho %}
{% endblock %}

{% block contenido -%}
    <table class="table tab-content table-hover table-striped records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Documentoidentidad</th>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Fecharegistro</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('administrador_configuracion_administrador_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.documentoidentidad }}</td>
                    <td>{{ entity.nombres }}</td>
                    <td>{{ entity.apellidos }}</td>
                    <td>{% if entity.fecharegistro %}{{ entity.fecharegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('administrador_configuracion_administrador_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('administrador_configuracion_administrador_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('administrador_configuracion_administrador_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
{% endblock %}
