{% endblock %}
{% block submenuizquierdo %}
{% endblock %}
{% block submenuderecho %}
{% endblock %}

{% block contenido -%}   


    <table class="table tab-content table-hover table-striped records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Documentoidentidad</th>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Edad</th>
                <th>Telefono</th>
                <th>Celular</th>      
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('administrador_educacion_estudiantenf_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.documentoidentidad }}</td>
                    <td>{{ entity.nombres }}</td>
                    <td>{{ entity.apellidos }}</td>
                    <td>{{ entity.edad }}</td>
                    <td>{{ entity.telefono }}</td>
                    <td>{{ entity.celular }}</td>            
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('administrador_educacion_estudiantenf_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('administrador_educacion_estudiantenf_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                            <li>
                                <a href="{{ path('administrador_educacion_estudiantenf_edit', { 'id': entity.id }) }}">matricular</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('administrador_educacion_estudiantenf_new') }}">
                Create a new entry
            </a>
