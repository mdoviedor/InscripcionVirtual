{% extends "::superadministrador.html.twig" %}
{% block titulo %}
    <h1>UsuarioCurso list</h1>
{% endblock %}
{% block submenuizquierdo %}
{% endblock %}
{% block submenuderecho %}
{% endblock %}

{% block contenido -%} 


    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Estado</th>
                <th>Fecharegistro</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
            {% for entity in entities %}
                <tr>
                    <td><a href="{{ path('administrador_educacion_usuariocurso_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                    <td>{{ entity.estado }}</td>
                    <td>{% if entity.fecharegistro %}{{ entity.fecharegistro|date('Y-m-d H:i:s') }}{% endif %}</td>
                    <td>
                        <ul>
                            <li>
                                <a href="{{ path('administrador_educacion_usuariocurso_show', { 'id': entity.id }) }}">show</a>
                            </li>
                            <li>
                                <a href="{{ path('administrador_educacion_usuariocurso_edit', { 'id': entity.id }) }}">edit</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('administrador_educacion_usuariocurso_new', {'id':'1'}) }}">
                Create a new entry
            </a>
        </li>
    </ul>
{% endblock %}
